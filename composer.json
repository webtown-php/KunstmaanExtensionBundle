{
    "name": "webtown/kunstmaan-extension-bundle",
    "type": "kunstmaan-bundle",
    "description": "Kunstmaan extension bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/webtown-php/KunstmaanExtensionBundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Krisztián Ferenczi",
            "email": "ferenczi.krisztian@gmail.com",
            "role": "Developer"
        },
        {
            "name": "László Somogyi",
            "email": "somogyi.laszlo@webtown.hu",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "symfony/symfony": "~2.7||~3",
        "fchris82/jms-translation-bundle-fork": "~1.2.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "suggest": {
        "ext-mongo": "*",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/mongodb-odm-bundle": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Webtown\\KunstmaanExtensionBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webtown\\KunstmaanExtensionBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
